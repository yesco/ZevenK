# make keymap from .lst files and open browser (on termux)

perl Words/mk-asdfg.pl Words/alpha-strokes.lst.edited  | sort > asdfg.lst

cat spell.map | perl mk-spell.pl | sort > spell.generated.lst

perl mk-json.pl *.lst > asdfg.json.js

(echo "<script>" ; \
 cat asdfg.json.js; \
 echo "</script>"; \
 cat sim.html \
) > run.html

termux-open run.html

